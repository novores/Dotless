colorscheme gruvbox
syntax on
filetype on
filetype indent on
set ruler
set number
set noshowmode
set autoindent smartindent
set encoding=utf-8
set termguicolors
let python_highlight_all=1
set relativenumber
set foldmethod=marker
set ignorecase
set smartcase
set scrolloff=10
set cursorline
set nowrap
filetype plugin indent on
COQnow --shut-up

"" Copy/Paste/Cut{{{
if has('unnamedplus')
  set clipboard=unnamed,unnamedplus
endif"}}}
